msgid "Author:\t\t"
msgstr "രചയിതാവ്:\t\t"

#: /var/www/pootle/sugarlabs/clones/GetBooks/GetIABooksActivity.py:663
msgid "Summary:\t"
msgstr "ചുരുക്കത്തില്‍:\t"

